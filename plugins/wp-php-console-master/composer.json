{
  "name": "unfulvio/wp-php-console",
  "description": "A WordPress implementation of PHP Console.",
  "type": "wordpress-plugin",
  "version": "1.4.1",
  "keywords": [
    "wordpress",
    "debug",
    "debugging",
    "development",
    "php-console",
    "console",
    "terminal",
    "command-line",
    "cli"
  ],
  "homepage": "https://github.com/unfulvio/wp-php-console",
  "license": "GPLv2.0+",
  "authors": [
    {
      "name": "Fulvio Notarstefano",
      "email": "fulvio.notarstefano@gmail.com",
      "homepage": "https://github.com/unfulvio",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "composer/installers": "v1.0.22",
    "php-console/php-console": "~3.0",
    "nekojira/wp-requirements": "^1.4"
  },
  "require-dev": {
    "phpunit/phpunit": "*@stable",
    "whatthejeff/nyancat-phpunit-resultprinter": "~1.2"
  },
  "scripts": {
    "post-install-cmd": [
      "xrstf\\Composer52\\Generator::onPostInstallCmd"
    ],
    "post-update-cmd": [
      "xrstf\\Composer52\\Generator::onPostInstallCmd"
    ],
    "post-autoload-dump": [
      "xrstf\\Composer52\\Generator::onPostInstallCmd"
    ]
  }
}
